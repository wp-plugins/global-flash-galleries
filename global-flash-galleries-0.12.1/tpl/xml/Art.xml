<?xml version="1.0" encoding="utf-8"?>
<gallery>
	<galleryName>Art Gallery</galleryName>
	<settings>
		<image
			align="{image.align}"
			cornerRadius="{image.cornerRadius}"
			scaleMode="{image.scaleMode}"
			transitionEffect="{image.transitionEffect}"
			transitionDuration="{image.transitionDuration}"
			useShadow="{image.useShadow}"
		/>
		<thumbBar
			useThumbBar="{thumbBar.useThumbBar}"
			position="{thumbBar.position}"
			scrollSpeed="{thumbBar.scrollSpeed}"
		/>
		<thumbnail
			width="{thumbnail.width}"
			height="{thumbnail.height}"
			cornerRadius="{thumbnail.cornerRadius}"
			alpha="{thumbnail.alpha}"
			outlineColor="0x{thumbnail.outlineColor}"
			outlineColorOnRollOver="0x{thumbnail.outlineColorOnRollOver}"
			outlineColorOnClick="0x{thumbnail.outlineColorOnClick}"
			useShadow="{thumbnail.useShadow}"
		/>
		<preview
			usePreview="{preview.usePreview}"
			alpha="{preview.alpha}"
			width="{preview.width}"
			height="{preview.height}"
			outlineColor="0x{preview.outlineColor}"
			useShadow="{preview.useShadow}"
			cornerRadius="{preview.cornerRadius}"
		/>
		<caption
			visibleMode="{caption.visibleMode}"
			position="{caption.position}"
			color="0x{caption.color}"
			bgColor="0x{caption.bgColor}"
			bgAlpha="{caption.bgAlpha}"
			fontName="{caption.fontName}"
			fontSize="{caption.fontSize}"
		/>
		<slideshow
			start="{slideshow.start}"
			delay="{slideshow.delay}"
			loop="{slideshow.loop}"
			stopAutoOnClick="{slideshow.stopAutoOnClick}"
		/>
		<preloader
			usePreloader="{preloader.usePreloader}"
			alpha="{preloader.alpha}"
		/>
		<buttons
			fullScreenButton="{buttons.fullScreenButton}"
			navigationButtons="{buttons.navigationButtons}"
			slideshowButton="{buttons.slideshowButton}"
		/>
		<sounds
			onRollOver="{sounds.onRollOver}"
			onClick="{sounds.onClick}"
		/>
		<picasa
			user="{picasa.user}"
			albumID="{picasa.albumID}"
		/>
		<flickr
			userID="{flickr.userID}"
			photosetID="{flickr.photosetID}"
			tags="{flickr.tags}"
		/>
		<loader>{loader}</loader>
	</settings>
	<items>{items}
	</items>
</gallery>